{
  "private": true,
  "scripts": {
    "build": "turbo run build --filter @plone/registry",
    "start": "turbo run start --filter @plone/volto",
    "start2": "turbo run start2",
    "dev": "turbo run dev2 --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "tsconfig": "workspace:*",
    "turbo": "latest"
  },
  "name": "my-turborepo",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@8.9.0"
}
